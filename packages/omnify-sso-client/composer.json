{
    "name": "famgia/omnify-sso-client",
    "description": "SSO Client Package for Laravel - Provides Role, Permission and RolePermission models with Omnify schema-driven development",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sso",
        "authentication",
        "authorization",
        "roles",
        "permissions",
        "omnify"
    ],
    "authors": [
        {
            "name": "Omnify Team",
            "email": "dev@famgia.com"
        }
    ],
    "homepage": "https://github.com/ecsol/omnify-sso-client",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Omnify\\SsoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnify\\SsoClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "omnify": {
            "schemas": "database/schemas"
        },
        "laravel": {
            "providers": [
                "Omnify\\SsoClient\\SsoClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}