{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@omnify/*": [
        "frontend/src/omnify/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "*.ts"
  ]
}
